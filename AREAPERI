#include <stdio.h>
int main(){
	int l, b, area, peri;
		scanf("%d\n %d", &l, &b);
	area = l*b;
	peri = 2*(l + b);
		if (area > peri){
			printf("Area\n");
			printf("%d", area);
		}
		else if (peri > area){
			printf("Peri\n");
			printf("%d", peri);			
		}
		else if (peri == area){
			printf("Eq\n%d\n", peri);
		}
	return 0;
}
